<% unless @nav.loose.empty? %>
	<div class="row">
		<div class="col-md-4">
			<% unless @nav.loose.start_bearings.nil? or @nav.loose.start_bearings.length == 0 %>
				<div class="well well-sm">
					<table class="table table-condensed">
						<thead><th>Front</th><th>Top</th><th></th></thead>
						<tbody>
							<tr>
								<td><% unless @nav.loose.start_bearings[0].nil? %>
									<span class="badge"><%= @nav.loose.start_bearings[0] %></span><br>
								<% end %></td>
								<td><% unless @nav.loose.start_bearings[1].nil? %>
									<span class="badge"><%= @nav.loose.start_bearings[1] %></span><br>
								<% end %></td>
								<td><% if @nav.loose.start? %>
									<span class="label label-success">Ship Valid!</span>
								<% elsif !@nav.loose.start_bearings[0].nil? and !@nav.loose.start_bearings[1].nil? %>
									<span class="label label-danger">Ship Not Valid!</span>
								<% end %></td>
							</tr>
						</tbody>
					</table>
				</div>
			<% end %>
		</div>
		<div class="col-md-4">
			<div class="well well-sm text-center">
				<table class="table table-condensed">
					<thead><th>Front</th><th>Top</th><th></th></thead>
					<tbody>
						<% @nav.midpoint_bearings.each do |bearing| %>
							<tr><td>
								<% if @nav.real_mid_piv? %><span class="badge"><% else %><span class="label label-info"><% end %>
								<%= bearing[0] unless bearing[0].nil? %></span></td>
								<td>
									<% if @nav.real_mid_rol? %><span class="badge"><% else %><span class="label label-info"><% end %>
									<%= bearing[1] unless bearing[1].nil? %></span></td></tr>
						<% end %>
					</tbody>
				</table>

