	<div class="row">
		<div class="col-md-6">
			<% unless @nav.loose.start_mid_piv.nil? and @nav.loose.start_mid_rol.nil? %>
				<div class="well well-sm text-center">
					Start to Mid:<br>
					Pivot: <% if @nav.loose.start_mid_piv %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %> <br>
					Roll: <% if @nav.loose.start_mid_rol %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %>
				</div>
			<% end %>
		</div>
		<div class="col-md-6">
			<% unless @nav.loose.mid_end_piv.nil? and @nav.loose.mid_end_rol.nil? %>
				<div class="well well-sm text-center">
					Middle to End:<br>
					Pivot: <% if @nav.loose.mid_end_piv %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %> <br>
					Roll: <% if @nav.loose.mid_end_rol %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %>
				</div>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<% unless @nav.loose.start_end_piv.nil? and @nav.loose.start_end_rol.nil? %>
				<div class="well well-sm text-center">
					Start to End:<br>
					Pivot: <% if @nav.loose.start_end_piv %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %> <br>
					Roll: <% if @nav.loose.start_end_rol %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %>
				</div>
			<% end %>
		</div>
	</div>
	<div class="row">
		<div class="col-md-6 col-md-offset-3">
			<% unless @nav.strict_piv.nil? and @nav.strict_rol.nil? %>
				<div class="well well-sm text-center">
					Strict Start to End:<br>
					Pivot: <% if @nav.strict_piv %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %> <br>
					Roll: <% if @nav.strict_rol %><span class="label label-success">Good!</span>
						<% else %><span class="label label-warning">Not Good!</span><% end %>
				</div>
			<% end %>
		</div>
	</div>
<% end %>
<%= render 'strictform' %>